<h1>Listing listings</h1>

<table>
  <thead>
    <tr>
      <th>Short description</th>
      <th>Long description</th>
      <th>Bedroom count</th>
      <th>Bathroom count</th>
      <th>Price</th>
      <th>Website</th>
      <th>Name</th>
      <th>Deposit</th>
      <th>Square feet</th>
      <th>Year built</th>
      <th>Pet policy</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.short_description %></td>
        <td><%= listing.long_description %></td>
        <td><%= listing.bedroom_count %></td>
        <td><%= listing.bathroom_count %></td>
        <td><%= listing.price %></td>
        <td><%= listing.website %></td>
        <td><%= listing.name %></td>
        <td><%= listing.deposit %></td>
        <td><%= listing.square_feet %></td>
        <td><%= listing.year_built %></td>
        <td><%= listing.pet_policy %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
